{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["cli/*", "lib/*", "scripts/**/*", "package.json"],
  "exclude": ["jscomp/test/**/*", "jscomp/gentype_tests/**/*"]
}
